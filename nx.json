{
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": { "dependencies": "*", "devDependencies": "*" },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "affected": { "defaultBase": "main" },
  "npmScope": "ballet",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "projects": {
    "dance-show": { "tags": ["type:app"] },
    "dance-show-e2e": {
      "tags": ["type:e2e"],
      "implicitDependencies": ["dance-show"]
    },
    "playground": { "tags": ["type:app"] },
    "playground-e2e": {
      "tags": ["type:e2e"],
      "implicitDependencies": ["playground"]
    },
    "user-service": { "tags": ["type:backend"] },
    "shared-storybook": { "tags": ["type:story"] },
    "shared-theme": { "tags": ["type:assets"] },
    "catalog": { "tags": ["type:lib"] },
    "rest": { "tags": ["type:lib"] },
    "shared-iu": { "tags": ["type:ui"] },
    "interface": { "tags": ["type:interface"] },
    "molecules": { "tags": ["type:components"] },
    "atoms": { "tags": ["type:components"] },
    "material": { "tags": ["type:components"] }
  }
}
